{
    "name": "SongWukong",
    "publisher": "dafengxiang",
    "displayName": "SongWukong",
    "description": "自动读取剪切板图片链接，自动获取图片尺寸，根据自定义模版补全信息，生成代码",
    "repository": "https://github.com/dafengxiang/vscode-extension",
    "version": "0.0.1",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:bgtocode"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "bgtocode",
            "title": "生成图片代码"
        }],
        "menus": {
            "editor/context": [{
                "command": "bgtocode"
            }]
        },
        "keybindings": [{
            "command": "bgtocode",
            "key": "ctrl+g",
            "mac": "cmd+g"
        }]
    },
    "scripts": {
        "vscode:prepublish": "pnpm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "pnpm run compile && pnpm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "16.x",
        "@types/vscode": "^1.73.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.26.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "jimp": "^0.16.2"
    }
}